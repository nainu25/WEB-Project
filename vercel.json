{
    "version": 2,
    "builds": [
      {
        "src": "realEstateBackend/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "index.html",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/realEstateBackend/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
}